@using MyMvcProjectTemplate.Web.Controllers

<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @(Html.ActionLink<HomeController>(HtmlConstants.AplicationBrandLinkName, c => c.Index(), null, new { @class = "navbar-brand" }))
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li>@(Html.ActionLink<HomeController>(HtmlConstants.HomeLinkName, c => c.Index()))</li>
                <li>@(Html.ActionLink<HomeController>(HtmlConstants.AboutLinkName, c => c.About()))</li>
                <li>@(Html.ActionLink<HomeController>(HtmlConstants.ContactLinkName, c => c.Contact()))</li>
            </ul>
            @Html.Partial("_LoginPartial")
        </div>
    </div>
</div>
